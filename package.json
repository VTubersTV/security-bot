{
	"name": "vtubers-tv-security",
	"version": "1.0.0",
	"description": "VTubers.TV Discord community verification system",
	"main": "src/index.ts",
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"format": "prettier --cache --write . --ignore-path .prettierignore",
		"build": "tsc && node scripts/build.js",
		"dev": "nodemon",
		"dev:debug": "nodemon src/index.ts /debug",
		"start": "node dist/index.js",
		"deploy-commands": "node scripts/deployCommands.js",
		"build:css": "tailwindcss -i ./src/server/public/css/input.css -o ./src/server/public/css/style.css --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/VTubersTV/security-bot.git"
	},
	"author": "VTubersTV",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/VTubersTV/security-bot/issues"
	},
	"homepage": "https://github.com/VTubersTV/security-bot#readme",
	"devDependencies": {
		"@types/cookie-parser": "^1.4.6",
		"@types/csurf": "^1.11.5",
		"@types/ejs": "^3.1.5",
		"@types/express": "^4.17.17",
		"@types/express-session": "^1.17.10",
		"@types/node": "^20.3.1",
		"autoprefixer": "^10.4.16",
		"nodemon": "^2.0.22",
		"postcss": "^8.4.31",
		"prettier": "^3.3.3",
		"tailwindcss": "^3.3.5",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"@napi-rs/canvas": "^0.1.69",
		"@tailwindcss/forms": "^0.5.10",
		"axios": "^1.9.0",
		"chalk": "^5.4.1",
		"cookie-parser": "^1.4.6",
		"csurf": "^1.11.0",
		"date-fns": "^4.1.0",
		"discord.js": "^14.11.0",
		"dotenv": "^16.5.0",
		"ejs": "^3.1.9",
		"express": "^4.18.2",
		"express-rate-limit": "^7.1.5",
		"express-session": "^1.17.3",
		"helmet": "^7.1.0",
		"mongoose": "^8.14.0",
		"zod": "^3.24.3"
	}
